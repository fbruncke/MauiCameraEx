<?xml version="1.0" encoding="UTF-8" ?>
<Shell
	x:Class="MauiCameraEx.AppShell"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:views="clr-namespace:MauiCameraEx.Views"
	xmlns:local="clr-namespace:MauiCameraEx">

	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Main"
		Icon="iconblank.png"
		ContentTemplate="{DataTemplate views:MainPage}"
		Route="MainPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Camera"
		Icon="iconsample.png"
		ContentTemplate="{DataTemplate views:CameraPage}"
		Route="CameraPage" />
	<ShellContent
		Shell.NavBarIsVisible="true"
		Title="Picture"
		Icon="iconsample.png"
		ContentTemplate="{DataTemplate views:PicturePage}"
		Route="PicturePage" />
</Shell>
